<mvc:View controllerName="com.aifa.sap.tilesdashboard.controller.Shippers"
    xmlns:mvc="sap.ui.core.mvc"
    xmlns="sap.m"
    xmlns:layout="sap.ui.layout">
    <Page title="Shippers" titleAlignment="Center">
      <customHeader>
            <Bar>
                <contentLeft>
                    <HBox alignItems="Start">
                        <Image src="data:image/png;base64," 
                          width="100px" height="35px" />
                    </HBox>
                </contentLeft>
                <contentMiddle>
                <HBox alignContent="Center">
                        <Title text="Shippers"/>
                        </HBox>
                </contentMiddle>
            </Bar>
        </customHeader>
        <IconTabBar id="iconTabBar2" selectedKey="" class="iconTabBar2">
    <items>
        <!-- Shippers List -->
        <IconTabFilter text="Shippers" key="Shippers" icon="sap-icon://shipping-status" class="customTabSpacing2">
            <SearchField width="30%" id="searchField2" showSearchButton="true" 
                liveChange="onSearchLiveChange2" search="onSearchPress2"/>
            <ScrollContainer width="100%" height="475px" vertical="true">
                <List id="ShippersList" mode="SingleSelectMaster" items="{/Shippers}" selectionChange="onShipperSelect">
                    <CustomListItem>
                        <VBox class="sapUiSmallMargin" >
                            <!-- Shipper Company Name -->
                            <Title text="{CompanyName}" level="H5" />
                            <HBox>
                                <Label text="Shipper ID: " width="100px"/>
                                <Text text="{ShipperID}" />
                            </HBox>
                        </VBox>
                    </CustomListItem>
                </List>    
            </ScrollContainer>                        
        </IconTabFilter>

        <IconTabSeparator />

        <!-- Shipper Details -->
        <IconTabFilter text="Shipper Details" key="ShipperDetails" icon="sap-icon://business-card" class="customTabSpacing" visible="true">
            <content>
                <VBox class="sapUiSmallMargin" >
                    <HBox>
                        <Label text="Shipper ID" width="150px"/>
                        <Text id="shipperID" text="" />
                    </HBox>

                    <HBox>
                        <Label text="Company Name" width="150px"/>
                        <Text id="companyName" text="" />
                    </HBox>

                    <HBox>
                        <Label text="Phone" width="150px"/>
                        <Text id="phone" text="" />
                    </HBox>

                    <HBox class="sapUiSmallMarginTop">
                        <Button text="Back" press="onBackToShippers2"/>
                    </HBox>
                </VBox>
            </content>
        </IconTabFilter>

    </items>
</IconTabBar>

    </Page>
    
</mvc:View>